let x = 5;
let y = 3;

//
console.log('x + y = ', x + y);  //

//
console.log('x - y = ', x - y);  //

//
console.log('x * y = ', x * y);  //

//
console.log('x / y = ', x / y);  //
//
console.log('x % y = ', x % y);   //

// increment-
console.log('++x = ', ++x); //
console.log('x++ = ', x++); //
console.log('x = ', x);     //

//
console.log('--x = ', --x); //
console.log('x-- = ', x--); //
console.log('x = ', x);     //

//
console.log('x ** y =', x ** y);

//
console.log(2 == 2); //
console.log(2 == '2'); //

//
console.log(3 != 2); //
console.log('hello' != 'Hello'); //

//
console.log(2 === 2); //
console.log(2 === '2'); // false

//
console.log(2 !== '2'); // true
console.log(2 !== 2); // false
